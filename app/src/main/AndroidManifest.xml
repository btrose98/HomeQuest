<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.homequest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HomeQuest">
        <activity
            android:name=".calendar"
            android:label="@string/title_activity_calendar"
            android:theme="@style/Theme.HomeQuest.NoActionBar"></activity>
        <activity
            android:name=".parentSettings"
            android:label="@string/title_activity_parent_settings" />
        <activity android:name=".registerActivity" />
        <activity android:name=".childHome" />
        <activity android:name=".parentHome" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>